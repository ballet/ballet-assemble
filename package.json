{
  "name": "ballet-submit",
  "version": "0.1.0",
  "description": "Submit ballet modules from within JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/HDI-Project/ballet-submit-jupyterlab",
  "bugs": {
    "url": "https://github.com/HDI-Project/ballet-submit-jupyterlab/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Micah Smith",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/HDI-Project/ballet-submit-jupyterlab.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/docregistry": "^1.2.1",
    "@jupyterlab/notebook": "^1.2.2",
    "@phosphor/disposable": "^1.3.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
